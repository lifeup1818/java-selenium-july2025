<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="regression">
  <test name="Test">
    <classes>
      <class name="com.lifeup.swaglab.test.LoginTest">
      	<methods>
      		<include name="loginWithValidCredentials"/>
